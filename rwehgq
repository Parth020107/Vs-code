                     }
                 });
                 A.on( 'a:popover:ajaxSuccess', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:success", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
                 A.on( 'a:popover:ajaxFail', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:failure", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
             });
        }
        var defaultLinkBehaviour = function() {
            document.getElementById("ad-feedback-text-desktop-ad-center-1").style.textDecoration = 'none';
            document.getElementById("ad-feedback-text-desktop-ad-center-1").style.color = '#555555';
            document.getElementById("ad-feedback-sprite-desktop-ad-center-1").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat scroll 0px 0px';
        }
window.changeLinkColor1e446cc0206148d58f910650cfded611 = changeLinkColor;
window.defaultLinkBehaviour1e446cc0206148d58f910650cfded611 = defaultLinkBehaviour;
window.onClick1e446cc0206148d58f910650cfded611 = onClick;
    } catch(ex) {
        if(window.ueLogError) {
            var additionalInfo = {
                logLevel : 'ERROR',
                attribution : 'Ad Feedback',
                message : 'Error in Feedback js '
            };
            window.ueLogError(ex, additionalInfo);
        }
    }
})(window, document, Math);</script><a data-a-modal="{&quot;url&quot;:&quot;/af/feedback-form?pl=%7B%22adPlacementMetaData%22%3A%7B%22adElementId%22%3A%22ape_Gateway_desktop-ad-center-1_desktop_placement%22%2C%22pageType%22%3A%22Gateway%22%2C%22pageUrl%22%3A%22aHR0cHM6Ly93d3cuYW1hem9uLmluL3JlZj1uYXZfbG9nbz8%3D%22%2C%22slotName%22%3A%22desktop-ad-center-1%22%7D%2C%22adCreativeMetaData%22%3A%7B%22adProgramId%22%3A%221027%22%2C%22adCreativeId%22%3A%220%22%2C%22adId%22%3A%22300079775109432%22%2C%22adCreativeDetails%22%3A%5B%5D%2C%22adNetwork%22%3A%22cs%22%7D%7D&amp;daFlg=false&amp;ie=UTF-8&quot;,     &quot;name&quot;:&quot;shared-placement-feedback-modal-desktop-ad-center-1&quot;,&quot;header&quot;:&quot;Leave feedback&quot;,&quot;width&quot;:&quot;460&quot;}" style="font-size: 11px; color: rgb(85, 85, 85); cursor: pointer; text-decoration: none;" data-action="a-modal" class="a-declarative" tabindex="0">
