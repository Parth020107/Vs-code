    <div id="ape_Gateway_right-2_desktop_placement_Feedback" style="position: relative; height: 20px; text-align: right; padding: 0px 5px; margin-top: 2px;"><script>(function (window, document, Math) {
    try {
        var changeLinkColor = function () {
            document.getElementById("ad-feedback-text-right-2").style.color = '#111111';
            document.getElementById("ad-feedback-sprite-right-2").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat 0px -12px';
        }
        var onClick = function() {
             P.when('A').execute( function(A) {
                 A.on( 'a:popover:beforeShow', function() {
                     if(window.uet) {
                         window.uet('bb', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
                 A.on( 'a:popover:ajaxSuccess', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:success", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
                 A.on( 'a:popover:ajaxFail', function() {
                     if(window.ue && window.ue.count) {
                         window.ue.count("adFeedback:desktop:feedbackForm:failure", 1);
                     }
                     if(window.uet) {
                         window.uet('be', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                     if(window.uex) {
                         window.uex('ld', 'adFeedback:desktkop:feedbackForm:time', {wb: 1});
                     }
                 });
             });
        }
        var defaultLinkBehaviour = function() {
            document.getElementById("ad-feedback-text-right-2").style.textDecoration = 'none';
            document.getElementById("ad-feedback-text-right-2").style.color = '#555555';
            document.getElementById("ad-feedback-sprite-right-2").style.background = 'transparent url("https://m.media-amazon.com/images/G/01/ad-feedback/info_icon_1Xsprite.png") no-repeat scroll 0px 0px';
        }
window.changeLinkColor8c1d3de7371c4607b6ef3ea030a6cfd0 = changeLinkColor;
window.defaultLinkBehaviour8c1d3de7371c4607b6ef3ea030a6cfd0 = defaultLinkBehaviour;
window.onClick8c1d3de7371c4607b6ef3ea030a6cfd0 = onClick;
    } catch(ex) {
        if(window.ueLogError) {
            var additionalInfo = {
                logLevel : 'ERROR',
                attribution : 'Ad Feedback',
                message : 'Error in Feedback js '
            };
            window.ueLogError(ex, additionalInfo);
        }
    }
